<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="hx.mall.dao.PmsProductCategoryDao">
    <insert id="insertBatch">
        insert into pms_product_category
        values(parent_id,name,level,product_count,product_unit,nav_status,show_status,sort,icon,keywords,description)
        <foreach collection="list" item="item" separator=",">
            (#{item.parent_id},#{item.name},#{item.product_count},#{item.product_unit},#{item.nav_status},#{item.sort},#{item.icon},#{item.keywords},#{item.description})
        </foreach>
    </insert>
</mapper>